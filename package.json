    "imap-simple": "^5.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.9.0",
    "ms": "^2.1.3",
    "natural": "^8.1.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "playwright": "^1.56.1",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rss-parser": "^3.13.0",
    "simple-git": "^3.30.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "systeminformation": "^5.27.11",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "xlsx": "^0.18.5",
