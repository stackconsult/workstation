    "@types/ioredis": "^5.0.0",
    "@types/ms": "^2.1.0",
    "@types/natural": "^5.1.5",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "d3": "^7.9.0",
    "dockerode": "^4.0.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^166.0.0",
