{
  "name": "stackbrowseragent",
  "version": "1.0.0",
  "description": "Browser Agent with JWT Authentication",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "mkdir -p dist/automation/db && cp src/automation/db/schema.sql dist/automation/db/",
    "build:chrome": "rm -rf build/chrome-extension && mkdir -p build/chrome-extension && cp -r chrome-extension/* build/chrome-extension/",
    "deploy:chrome": "./scripts/deploy-chrome-extension.sh",
    "build:downloads": "node scripts/build-downloads.js",
    "generate:manifest": "node scripts/generate-manifest.js",
    "test:chrome": "node scripts/test-chrome-extension.js",
    "update:agent-status": "node scripts/update-agent-status.js",
    "update:agent-status:dry-run": "node scripts/update-agent-status.js --dry-run",
    "start": "node dist/index.js",
    "verify": "./scripts/verify-startup.sh",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "precommit": "./scripts/pre-commit-check.sh",
    "validate": "npm run lint && npm run build && npm test",
    "test:coverage-check": "npm test && node scripts/coverage-scaling.js check",
    "coverage:report": "node scripts/coverage-scaling.js report",
    "coverage:update-baseline": "node scripts/coverage-scaling.js update-baseline",
    "audit-ci": "audit-ci --moderate",
    "prepublishOnly": "npm run lint && npm run build",
    "deploy:builder": "./scripts/deploy-workflow-builder.sh",
    "deploy:all": "./scripts/deploy-all.sh",
    "test:integration:chrome-builder": "node scripts/test-integration.js",
    "agent1:setup": "./agents/agent1/run-build-setup.sh",
    "agent2:setup": "./agents/agent2/run-build-setup.sh",
    "agent3:setup": "./agents/agent3/run-build-setup.sh",
    "agent4:setup": "./agents/agent4/run-build-setup.sh",
    "agent5:setup": "./agents/agent5/run-build-setup.sh",
    "agent6:setup": "./agents/agent6/run-build-setup.sh",
    "agent7:weekly": "./agents/agent7/run-weekly-security.sh",
    "agent8:build": "cd agents/agent8 && npm install && npm run build",
    "agent8:assess": "cd agents/agent8 && npm run assess",
    "agent8:weekly": "./agents/agent8/run-weekly-assessment.sh",
    "agent9:build": "cd agents/agent9 && npm install && npm run build",
    "agent9:optimize": "cd agents/agent9 && npm run optimize",
    "agent9:weekly": "./agents/agent9/run-weekly-optimization.sh",
    "agent10:build": "cd agents/agent10 && npm install && npm run build",
    "agent10:validate": "cd agents/agent10 && npm run validate",
    "agent10:weekly": "./agents/agent10/run-weekly-guard-rails.sh",
    "agent11:build": "cd agents/agent11 && npm install && npm run build",
    "agent11:analyze": "cd agents/agent11 && npm run analyze",
    "agent11:weekly": "./agents/agent11/run-weekly-analytics.sh",
    "agent12:build": "cd agents/agent12 && npm install && npm run build",
    "agent12:qa": "cd agents/agent12 && npm run qa",
    "agent12:weekly": "./agents/agent12/run-weekly-qa.sh",
    "automation:install": "bash .automation/install-autonomous-system.sh",
    "automation:trigger": "bash .automation/trigger-cycle-now.sh",
    "automation:health": "bash .automation/check-cycle-health.sh",
    "build-setup:all": "npm run agent1:setup && npm run agent2:setup && npm run agent3:setup && npm run agent4:setup && npm run agent5:setup && npm run agent6:setup",
    "agent-builder:build": "ts-node scripts/agent-builder/agent-builder.ts --agent",
    "agent-builder:build-all": "ts-node scripts/agent-builder/agent-builder.ts --all",
    "orchestrator:deploy": "ts-node scripts/agent-builder/orchestrator.ts --agent",
    "orchestrator:deploy-all": "ts-node scripts/agent-builder/orchestrator.ts --all",
    "orchestrator:rollback": "ts-node scripts/agent-builder/orchestrator.ts --rollback",
    "agent17:build": "cd agents/agent17 && npm install && npm run build",
    "agent17:test": "cd agents/agent17 && npm test",
    "agent17:generate": "cd agents/agent17 && npm run generate",
    "stats:analyze": "npx ts-node src/scripts/webpage-stats-analyzer.ts",
    "stats:verify": "./scripts/verify-stats.sh"
  },
  "overrides": {
    "js-yaml": "^4.1.1"
  },
  "keywords": [
    "browser",
    "automation",
    "agent",
    "jwt",
    "express",
    "typescript",
    "authentication",
    "api",
    "rest",
    "nodejs"
  ],
  "author": "stackconsult",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackconsult/stackBrowserAgent.git"
  },
  "bugs": {
    "url": "https://github.com/stackconsult/stackBrowserAgent/issues"
  },
  "homepage": "https://github.com/stackconsult/stackBrowserAgent#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "API.md",
    "ARCHITECTURE.md",
    "CHANGELOG.md"
  ],
  "dependencies": {
