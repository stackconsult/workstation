{
  "name": "stackbrowseragent",
  "version": "1.0.0",
  "description": "Browser Agent with JWT Authentication",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "audit-ci": "audit-ci --moderate",
    "prepublishOnly": "npm run lint && npm run build",
    "agent8:build": "cd agents/agent8 && npm install && npm run build",
    "agent8:assess": "cd agents/agent8 && npm run assess",
    "agent8:weekly": "./agents/agent8/run-weekly-assessment.sh"
  },
  "overrides": {
    "js-yaml": "^4.1.1"
  },
  "keywords": [
    "browser",
    "automation",
    "agent",
    "jwt",
    "express",
    "typescript",
    "authentication",
    "api",
    "rest",
    "nodejs"
  ],
  "author": "stackconsult",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackconsult/stackBrowserAgent.git"
  },
  "bugs": {
    "url": "https://github.com/stackconsult/stackBrowserAgent/issues"
  },
  "homepage": "https://github.com/stackconsult/stackBrowserAgent#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "API.md",
    "ARCHITECTURE.md",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@types/ms": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.25",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "audit-ci": "^7.1.0",
    "eslint": "^9.0.0",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.46.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
