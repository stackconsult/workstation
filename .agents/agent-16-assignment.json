  "api_endpoints": {
    "base_url": "http://localhost:3016",
    "authentication": "JWT Bearer Token",
    "endpoints": [
      {
        "method": "GET",
        "path": "/health",
        "description": "Health check for Agent-16",
        "auth_required": false,
        "response_example": {
          "status": "healthy",
          "timestamp": "2024-11-19T12:00:00Z",
          "uptime": 3600,
          "container": "mcp-16-data",
          "version": "1.0.0"
        }
      },
      {
        "method": "GET",
        "path": "/api/containers/status",
        "description": "Get status of all MCP containers",
        "auth_required": true,
        "response_example": {
          "containers": [
            {
              "name": "mcp-01-selector",
              "status": "running",
              "health": "healthy",
              "port": 3001,
              "uptime": 7200
            }
          ],
          "total": 20,
          "healthy": 20,
          "unhealthy": 0,
          "stopped": 0
        }
      },
      {
        "method": "GET",
        "path": "/api/containers/:name/health",
        "description": "Check health of specific container",
        "auth_required": true,
        "parameters": {
          "name": "Container name (e.g., mcp-01-selector)"
        },
        "response_example": {
          "container": "mcp-01-selector",
          "status": "healthy",
          "last_check": "2024-11-19T12:00:00Z",
          "uptime": 7200,
          "health_checks_passed": 240,
          "health_checks_failed": 0
        }
      },
      {
        "method": "POST",
        "path": "/api/containers/peelback",
        "description": "Trigger container rollback to previous version",
        "auth_required": true,
        "request_body": {
          "container": "mcp-01-selector",
          "tag": "v1.0.0",
          "verify_health": true,
          "force": false
        },
        "response_example": {
          "success": true,
          "operation_id": "peelback-20241119-123456",
          "container": "mcp-01-selector",
          "tag": "v1.0.0",
          "status": "in_progress",
          "estimated_time": 60
        }
      },
      {
        "method": "GET",
        "path": "/api/containers/peelback/status/:id",
        "description": "Check status of peelback operation",
        "auth_required": true,
        "parameters": {
          "id": "Operation ID from peelback request"
        },
        "response_example": {
          "operation_id": "peelback-20241119-123456",
          "status": "completed",
          "container": "mcp-01-selector",
          "tag": "v1.0.0",
          "started_at": "2024-11-19T12:00:00Z",
          "completed_at": "2024-11-19T12:01:30Z",
          "duration": 90,
          "health_check_passed": true
        }
      },
      {
        "method": "POST",
        "path": "/api/github/push-branch",
        "description": "Push branch to GitHub repository",
        "auth_required": true,
        "request_body": {
          "branch": "feature/new-mcp-container",
          "message": "Add new MCP container configuration",
          "files": [
            "docker-compose.mcp.yml"
          ]
        },
        "response_example": {
          "success": true,
          "branch": "feature/new-mcp-container",
          "commit": "a1b2c3d4",
          "url": "https://github.com/creditXcredit/workstation/tree/feature/new-mcp-container"
        }
      },
      {
        "method": "POST",
        "path": "/api/github/sync",
        "description": "Sync repository with GitHub",
        "auth_required": true,
        "request_body": {
          "branch": "main",
          "pull": true,
          "push": false
        },
        "response_example": {
          "success": true,
          "branch": "main",
          "commits_pulled": 5,
          "commits_pushed": 0,
          "timestamp": "2024-11-19T12:00:00Z"
        }
      },
      {
        "method": "GET",
        "path": "/api/metrics",
        "description": "Get container metrics and performance data",
        "auth_required": true,
        "response_example": {
          "containers": 20,
          "total_requests": 15000,
          "avg_response_time_ms": 45,
          "uptime_percentage": 99.9,
          "last_24h": {
            "requests": 2500,
            "errors": 3,
            "avg_response_time_ms": 42
          }
        }
      }
    ]
  },
  "environment_variables": {
    "required": [
      "MCP_MANAGER_AGENT",
      "MCP_CONTAINER_COUNT",
      "NODE_ENV",
      "JWT_SECRET"
    ],
    "optional": [
      "GITHUB_TOKEN",
      "MCP_PORT_OFFSET",
      "LOG_LEVEL",
      "DEBUG"
    ],
    "example": {
      "MCP_MANAGER_AGENT": "agent-16",
      "MCP_CONTAINER_COUNT": "20",
      "MCP_PORT_OFFSET": "3000",
      "NODE_ENV": "production",
      "LOG_LEVEL": "info"
    }
  },
  "health_monitoring": {
    "check_interval": "30s",
    "check_timeout": "10s",
    "retries_before_unhealthy": 3,
    "alerts": {
      "container_down": {
        "severity": "critical",
        "action": "Auto-restart if restart policy allows"
      },
      "container_unhealthy": {
        "severity": "high",
        "action": "Log warning and monitor"
      },
      "high_error_rate": {
        "severity": "medium",
        "action": "Alert manager for review"
      }
    }
  },
  "peelback_policies": {
    "auto_peelback_on_failure": false,
    "backup_before_peelback": true,
    "verify_health_after_peelback": true,
    "rollback_on_peelback_failure": true,
    "default_stable_tag": "stable",
    "max_peelback_attempts": 3
  },
  "security": {
    "authentication": "JWT Bearer Token",
    "rate_limiting": {
      "enabled": true,
      "requests_per_minute": 100
    },
    "cors": {
      "enabled": true,
      "allowed_origins": [
        "http://localhost:3000",
        "https://workstation.example.com"
      ]
    },
    "secrets_management": {
      "github_token": "Store in environment variables, never commit",
      "jwt_secret": "Generate using crypto.randomBytes(32).toString('hex')"
    }
  },
  "logging": {
    "log_level": "info",
    "log_format": "json",
    "log_destinations": [
      "/app/logs/agent-16.log",
      "/var/log/mcp/manager.log"
    ],
    "log_rotation": {
      "enabled": true,
      "max_size": "100M",
      "max_files": 10
    }
