  "assignment": {
    "title": "GitHub Integration and Code Automation",
    "description": "Agent 16 provides comprehensive GitHub integration, code analysis, and automated coding capabilities through the Model Context Protocol (MCP).",
    "assignedDate": "2025-11-19",
    "priority": "high",
    "category": "development-automation"
  },
  "capabilities": [
    {
      "name": "github-operations",
      "description": "Full GitHub API integration for repository management",
      "endpoints": [
        "/api/github/repos",
        "/api/github/pulls",
        "/api/github/issues",
        "/api/github/commits"
      ]
    },
    {
      "name": "code-analysis",
      "description": "Static code analysis and quality checking",
      "tools": ["eslint", "prettier", "typescript-compiler"]
    },
    {
      "name": "automated-reviews",
      "description": "Automated pull request reviews and suggestions",
      "features": ["style-check", "best-practices", "security-scan"]
    },
    {
      "name": "data-processing",
      "description": "ETL pipelines and data transformation",
      "formats": ["json", "csv", "yaml", "xml"]
    },
    {
      "name": "mcp-server",
      "description": "Model Context Protocol server for AI integration",
      "port": 3016,
      "health_endpoint": "/health"
    }
  ],
  "dependencies": {
    "required": [
      "GITHUB_TOKEN"
    ],
    "optional": [
      "SLACK_WEBHOOK_URL",
      "SENTRY_DSN"
    ]
  },
  "configuration": {
    "mcp_container": "mcp-16-data-processing",
    "docker_image": "workstation-mcp-16:latest",
    "port_mapping": "3016:3000",
    "health_check": {
      "enabled": true,
      "interval": "30s",
      "timeout": "10s",
      "retries": 3
    },
    "restart_policy": "unless-stopped"
  },
  "integration": {
    "coding_agent_path": "tools/coding-agent",
    "dockerfile": "tools/coding-agent/Dockerfile",
    "entry_point": "src/index.ts"
  },
  "deployment": {
    "compose_file": "mcp-containers/docker-compose.mcp.yml",
    "service_name": "mcp-16-data-processing",
    "rollback_script": ".docker/peelback.sh"
  },
  "documentation": {
    "readme": "tools/coding-agent/README.md",
    "api_docs": "docs/agent-16-api.md",
    "mcp_guide": "mcp-containers/README.md"
  },
  "tasks": [
