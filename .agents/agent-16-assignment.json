  "metrics": {
    "uptime_target": "99.9%",
    "response_time_target": "< 500ms",
    "success_rate_target": "> 95%"
  },
  "alerts": {
    "health_check_failures": {
      "threshold": 3,
      "action": "restart"
    },
    "high_error_rate": {
      "threshold": "5%",
      "action": "notify"
    },
    "slow_response_time": {
