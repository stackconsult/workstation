{
  "id": "etl-data-pipeline",
  "name": "ETL Data Pipeline",
  "description": "Extract, Transform, Load data pipeline. Pulls data from multiple sources, cleanses and transforms it, validates quality, and loads into data warehouse with error handling and monitoring.",
  "category": "data-processing",
  "icon": "ðŸ”„",
  "tags": ["data", "etl", "pipeline", "transformation", "analytics"],
  "complexity": "advanced",
  "estimatedDuration": "30-120 minutes",
  "createdAt": "2025-11-24T00:00:00Z",
  "updatedAt": "2025-11-24T00:00:00Z",
  "nodes": [
    {"id": "node-1", "type": "start", "label": "Start ETL", "x": 100, "y": 100},
    {"id": "node-2", "type": "extract_db", "label": "Extract from SQL DB", "x": 50, "y": 200, "params": {"query": "SELECT * FROM orders WHERE updated_at > {{last_run}}"}},
    {"id": "node-3", "type": "extract_api", "label": "Extract from API", "x": 150, "y": 200, "params": {"endpoint": "{{env.API_URL}}/data", "pagination": true}},
    {"id": "node-4", "type": "extract_csv", "label": "Extract from CSV/S3", "x": 250, "y": 200, "params": {"bucket": "data-lake", "pattern": "raw/{{date}}/*.csv"}},
    {"id": "node-5", "type": "merge_data", "label": "Merge Data Sources", "x": 150, "y": 300, "params": {"joinKey": "id", "strategy": "outer"}},
    {"id": "node-6", "type": "cleanse_data", "label": "Data Cleansing", "x": 150, "y": 400, "params": {"operations": ["remove_nulls", "trim_whitespace", "deduplicate", "standardize_formats"]}},
    {"id": "node-7", "type": "transform_data", "label": "Transform Data", "x": 150, "y": 500, "params": {"transformations": ["calculate_derived_fields", "normalize_values", "enrich_data"]}},
    {"id": "node-8", "type": "validate_quality", "label": "Data Quality Checks", "x": 150, "y": 600, "params": {"rules": ["completeness", "accuracy", "consistency", "uniqueness"]}},
    {"id": "node-9", "type": "condition", "label": "Quality Pass?", "x": 150, "y": 700, "params": {"condition": "quality_score > 95"}},
    {"id": "node-10", "type": "load_warehouse", "label": "Load to Data Warehouse", "x": 250, "y": 800, "params": {"table": "analytics.orders", "mode": "upsert"}},
    {"id": "node-11", "type": "quarantine_data", "label": "Quarantine Failed Records", "x": 50, "y": 800, "params": {"destination": "s3://quarantine/{{date}}/"}},
    {"id": "node-12", "type": "update_metadata", "label": "Update Data Catalog", "x": 200, "y": 900, "params": {"catalog": "glue", "updateStats": true}},
    {"id": "node-13", "type": "generate_report", "label": "Generate ETL Report", "x": 150, "y": 1000, "params": {"metrics": ["records_processed", "quality_score", "duration"]}},
    {"id": "node-14", "type": "end", "label": "End", "x": 150, "y": 1100}
  ],
  "connections": [
    {"id": "conn-1", "source": "node-1", "target": "node-2"},
    {"id": "conn-2", "source": "node-1", "target": "node-3"},
    {"id": "conn-3", "source": "node-1", "target": "node-4"},
    {"id": "conn-4", "source": "node-2", "target": "node-5"},
    {"id": "conn-5", "source": "node-3", "target": "node-5"},
    {"id": "conn-6", "source": "node-4", "target": "node-5"},
    {"id": "conn-7", "source": "node-5", "target": "node-6"},
    {"id": "conn-8", "source": "node-6", "target": "node-7"},
    {"id": "conn-9", "source": "node-7", "target": "node-8"},
    {"id": "conn-10", "source": "node-8", "target": "node-9"},
    {"id": "conn-11", "source": "node-9", "target": "node-10", "label": "true"},
    {"id": "conn-12", "source": "node-9", "target": "node-11", "label": "false"},
    {"id": "conn-13", "source": "node-10", "target": "node-12"},
    {"id": "conn-14", "source": "node-11", "target": "node-12"},
    {"id": "conn-15", "source": "node-12", "target": "node-13"},
    {"id": "conn-16", "source": "node-13", "target": "node-14"}
  ]
}
