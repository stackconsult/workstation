      "id": "node-1",
      "type": "start",
      "label": "Start",
      "x": 100,
      "y": 100
    },
    {
      "id": "node-2",
      "type": "navigate",
      "label": "Go to Login Page",
      "x": 100,
      "y": 200,
      "params": {
        "url": "https://social-platform.com/login",
        "waitUntil": "domcontentloaded"
      }
    },
    {
      "id": "node-3",
      "type": "fill",
      "label": "Enter Credentials",
      "x": 100,
      "y": 300,
      "params": {
        "fields": {
          "#username": "your_username",
          "#password": "your_password"
        }
      }
    },
    {
      "id": "node-4",
      "type": "click",
      "label": "Click Login",
      "x": 100,
      "y": 400,
      "params": {
        "selector": "button[type='submit']"
      }
    },
    {
      "id": "node-5",
      "type": "wait",
      "label": "Wait for Dashboard",
      "x": 100,
      "y": 500,
      "params": {
        "type": "selector",
        "selector": ".dashboard",
        "timeout": 10000
      }
    },
    {
      "id": "node-6",
      "type": "click",
      "label": "Click New Post",
      "x": 100,
      "y": 600,
      "params": {
        "selector": ".create-post-button"
      }
    },
    {
      "id": "node-7",
      "type": "fill",
      "label": "Write Post Content",
      "x": 100,
      "y": 700,
      "params": {
        "fields": {
          ".post-textarea": "Your post content here! #automation #productivity"
        }
      }
    },
    {
      "id": "node-8",
      "type": "click",
      "label": "Publish Post",
      "x": 100,
      "y": 800,
      "params": {
        "selector": ".publish-button"
      }
    },
    {
      "id": "node-9",
      "type": "wait",
      "label": "Wait for Confirmation",
      "x": 100,
      "y": 900,
      "params": {
        "type": "selector",
        "selector": ".success-message",
        "timeout": 5000
      }
    },
    {
      "id": "node-10",
      "type": "click",
      "label": "Logout",
      "x": 100,
      "y": 1000,
      "params": {
        "selector": ".logout-button"
      }
